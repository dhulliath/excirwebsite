<!DOCTYPE html>
<html>

<head>
    <title>{{global.site.title}}{{#if _page.title}} - {{_page.title}}{{/if}}</title>
    {{#if _page.styling.theme.stylesheets}} {{#each _page.styling.theme.stylesheets}} {{#if src}}
    <link rel="stylesheet" href="{{src}}">{{/if}} {{#if cascadingstylesheet}}
    <link rel="stylesheet" href="{{cascadingstylesheet}}">{{/if}} {{/each}} {{else}} {{#each global.styling.page_styles.standard.stylesheets}} {{#if src}}
    <link rel="stylesheet" href="{{src}}">{{/if}} {{#if cascadingstylesheet}}
    <link rel="stylesheet" href="{{cascadingstylesheet}}">{{/if}} {{/each}} {{/if}} {{>seo}}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">
</head>

<body style="padding-top:100px;">
    
    <div class="bosstop" style="position: relative;">
        <a href="/" style="display:block; position: absolute; left:0;width:50%;">
            <img src="{{global.site.image.src}}" width="100%">
        </a>
        <div style="display:block; position:absolute;right:0;width:50%;">
            {{#if _page.title}}{{_page.title}}{{/if}}{{#if _blog.title}}{{_blog.title}}{{/if}}
        </div>
    </div>
    <!--<header class="regular">
        <a class="image" href="/">
            <img src="{{global.site.image.src}}" width="100%">
        </a> {{#if _page.title}}
        <div class="lineAround">
            {{_page.title}}
        </div>
        {{/if}} {{#if _blog.title}}
        <div class="lineAround">
            {{_blog.title}}
        </div>
        {{/if}}
    </header>-->
    <nav>
        <div>
            <label for="drop" class="toggle">&#9776; {{#if _page.title}}{{_page.title}}{{else}}Menu{{/if}} </label>
            <input type="checkbox" id="drop" /> {{#navlist 'main'}}
            <ul class="menu">
                {{#each this}}
                <li>
                    {{#if subgroup}}<label for="drop-{{subgroup}}" class="toggle">{{label}} +</label>{{/if}}
                    <a href="{{#if url}}{{url}}{{else}}javascript:void(){{/if}}">{{label}}</a> {{#if subgroup}}
                    <input type="checkbox" id="drop-{{subgroup}}" /> {{#navlist subgroup}}
                    <ul class="hidden">
                        {{#each this}}

                        <a href="{{url}}">
                            <li>{{label}}</li>
                        </a>

                        {{/each}}
                    </ul>
                    {{/navlist}} {{/if}}
                </li>
                {{/each}}
            </ul>
            {{/navlist}}
        </div>
    </nav>
    <header>

    </header>
    <main>
        <div class="goldwidget"></div>

        <article>
            {{>@partial-block}} {{>_sections}}
        </article>
    </main>
    <footer>
        {{{marked global.site.footer}}}
    </footer>
</body>
<script data-main="/assets/js/main.js" src="/assets/js/require.js"></script>
{{#if _page.styling.theme.stylesheets}} {{#each _page.styling.theme.stylesheets}} {{#if javascript}}
<script src="{{javascript}}"></script>{{/if}} {{/each}} {{else}} {{#each global.styling.page_styles.standard.stylesheets}} {{#if javascript}}
<script src="{{javascript}}"></script>{{/if}} {{/each}} {{/if}}

</html>